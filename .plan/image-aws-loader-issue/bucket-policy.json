{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyDirectPublicRead",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::kan-storage-loveganesh-lv/*",
            "Condition": {
                "StringNotEquals": {
                    "s3:authType": "REST-QUERY-STRING"
                }
            }
        },
        {
            "Sid": "AllowPresignedURLAccess",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::kan-storage-loveganesh-lv/*",
            "Condition": {
                "StringEquals": {
                    "s3:authType": "REST-QUERY-STRING"
                }
            }
        }
    ]
}